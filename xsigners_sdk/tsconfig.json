{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "../",
      "baseUrl": "./",
      "paths": {
        "@/*": [
          "./src/*"
        ]
      },
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  